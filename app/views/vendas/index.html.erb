<p id="notice"><%= notice %></p>

<h1>Vendas</h1>

<table>
  <thead>
    <tr>
      <th>Cvalor total</th>
      <th></th>
      <th>Keys</th>
      <th>Clientes</th>
      <th>Produtos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendas.each do |venda| %>
      <tr>
        <td><%= venda.cvalor_total %></td>
        <td><%= venda. %></td>
        <td><%= venda.keys %></td>
        <td><%= venda.clientes_id %></td>
        <td><%= venda.produtos_id %></td>
        <td><%= link_to 'Show', venda %></td>
        <td><%= link_to 'Edit', edit_venda_path(venda) %></td>
        <td><%= link_to 'Destroy', venda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venda', new_venda_path %>
